<h1>Match: <%= @match.pair_name %> </h1>

<%= link_to 'Edit this Match', edit_match_path(@match), class: 'btn' %>

<div class='row'>
	<div class='col s12 m4 offset-m1'>
		<h4> Picture 1 </h4>
		<%= image_tag(@match.pic1.url(:standard)) %>
	</div>

	<div class='col s12 m4 offset-m1'>
		<h4> Picture 2 </h4>
		<%= image_tag(@match.pic2.url(:standard)) %>
	</div>
</div>